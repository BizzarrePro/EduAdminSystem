<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator
1.0.2//EN"
"http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="num">
		<field-validator type="regex">
			<param name="expression"><![CDATA[(\d{10}|\d{5})]]></param>
			<message>The num must be 5 bits or 10 bits</message>
		</field-validator>
	</field>
	<field name="password">
		<field-validator type="requiredstring">
			<message>The password is required</message>
		</field-validator>
	</field>
</validators>
		
